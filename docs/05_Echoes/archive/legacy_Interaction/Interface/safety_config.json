{
  "maturity": "INFANT",
  "action_limits": {
    "INFANT": {
      "system_control": {
        "mouse_move": "restricted",
        "keyboard_input": "blocked",
        "screen_access": "monitored"
      },
      "file_access": {
        "read": "restricted",
        "write": "blocked",
        "execute": "blocked"
      },
      "network_access": {
        "outbound": "blocked",
        "inbound": "monitored"
      }
    },
    "TODDLER": {
      "system_control": {
        "mouse_move": "monitored",
        "keyboard_input": "restricted",
        "screen_access": "allowed"
      },
      "file_access": {
        "read": "allowed",
        "write": "restricted",
        "execute": "blocked"
      }
    }
  },
  "safety_rules": {
    "system_control": {
      "mouse_center_limit": {
        "max_attempts": 3,
        "cooldown_period": 300,
        "violation_action": "temporary_block"
      },
      "rapid_action_limit": {
        "max_actions_per_minute": 30,
        "violation_action": "warning"
      }
    },
    "file_access": {
      "protected_paths": [
        "/system/",
        "/config/",
        "safety_config.json"
      ],
      "max_file_size": 1048576,
      "allowed_extensions": [
        ".txt",
        ".json",
        ".log"
      ]
    }
  },
  "override_policy": {
    "file_access": {
      "read": "allow",
      "write": "sandbox",
      "execute": "deny"
    },
    "network_access": {
      "outbound": "sandbox",
      "inbound": "sandbox"
    },
    "process_control": {
      "spawn": "sandbox",
      "kill": "deny"
    }
  }
}